  const userProDetails = async (email) => {
    console.log(email, 'check');

    const  data  = await getProDetails.get({
      email: email.toLowerCase()
    });
    if (data && data != null) {
      setCurrentPlan(data.plan);
      setPlanStatus(data.plan_status);
      dispatch(setCounters(data.counter_ai_report, data.counter_interactions));
      console.log(data,'pppp');

    }
    console.log(data);
  }
